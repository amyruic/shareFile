ARRSIZE?=1048576
STRIDE?=23876317
ITERATIONS?=1
.SILENT: 

all: q1 q3a q3b
q1: q1.c
	gcc -DITERATIONS=$(ITERATIONS) -DSTRIDE=$(STRIDE) -DARRSIZE=$(ARRSIZE) q1.c -o q1 -O3 


q3a: q3a.c
	gcc -DITERATIONS=$(ITERATIONS) -DSTRIDE=1544192 -DARRSIZE=$(ARRSIZE) q3a.c -o q3a -O3 
q3b: q3b.c
	gcc -DITERATIONS=$(ITERATIONS) -DSTRIDE=1544192 -DARRSIZE=$(ARRSIZE) q3b.c -o q3b -O3 


clean:
	rm -f q1 q2a q2b q3b q3a q4
